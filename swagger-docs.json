{
  "openapi": "3.0.2",
  "info": {
    "description": "Starter API docs",
    "version": "0.0.1",
    "title": "Seed Swagger API",
    "contact": {
      "email": "you@your-company.com"
    },
  },
  "tags": [
    {
      "name": "general",
      "description": "general and metrics command"
    },
    {
      "name": "api",
      "description": "general apis"
    },
    {
      "name" : "admin",
      "description": "admin functions"
    },
    {
      "name" : "database",
      "description": "database functions"
    }
  ],
  "paths": {
    "/health": {
      "get": {
        "tags": [
          "general"
        ],
        "summary": "health check",
        "operationId": "healthCheckId",
        "description": "Check service health. is it alive? is it healthy?\n",
        "responses": {
          "200": {
            "description": "search results matching criteria",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schema/healthSchema"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/hello": {
      "get": {
        "tags": [
          "api"
        ],
        "summary": "hello",
        "operationId": "helloId",
        "description": "GET a hello",
        "responses": {
          "200": {
            "description": "helooo"
          }
        }
      }
    },
    "/v1/bye": {
      "get": {
        "tags": [
          "api"
        ],
        "summary": "bye",
        "operationId": "byeId",
        "description": "GET a bye",
        "responses": {
          "200": {
            "description": "byee"
          }
        }
      }
    }
  },
    "components": {
      "schemas": {
        "healthCheckSchema": {
          "required": [
            "status",
            "uptime"
          ],
          "properties": {
            "status": {
              "type": "string",
              "example": "UP"
            },
            "uptime": {
              "type": "string",
              "format": "time",
              "example": "1 day 2 hours"
            }
          }
        }
      }      
    }
}